gradle_group_tasks=( 'assemble:Assembles the outputs of this project. [jar]' 'build:Assembles and tests this project. [assemble, check]' 'buildDependents:Assembles and tests this project and all projects that depend on it. [build]' 'buildNeeded:Assembles and tests this project and all projects it depends on. [build]' 'classes:Assembles classes '\''main'\''.' 'clean:Deletes the build directory.' 'jar:Assembles a jar archive containing the main classes. [classes]' 'testClasses:Assembles classes '\''test'\''. [classes]' 'init:Initializes a new Gradle build. [incubating]' 'wrapper:Generates Gradle wrapper files. [incubating]' 'javadoc:Generates Javadoc API documentation for the main source code. [classes]' 'dependencies:Displays all dependencies declared in root project '\''demo01'\''.' 'dependencyInsight:Displays the insight into a specific dependency in root project '\''demo01'\''.' 'help:Displays a help message' 'projects:Displays the sub-projects of root project '\''demo01'\''.' 'properties:Displays the properties of root project '\''demo01'\''.' 'tasks:Displays the tasks runnable from root project '\''demo01'\''.' 'check:Runs all checks. [test]' 'test:Runs the unit tests. [classes, testClasses]' )
gradle_all_tasks=( 'compileJava:Compiles Java source '\''main:java'\''.' 'processResources:Processes resources '\''main:resources'\''.' 'compileTestJava:Compiles Java source '\''test:java'\''.' 'processTestResources:Processes resources '\''test:resources'\''.' )
